{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://your-server-url.vercel.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install"
}
